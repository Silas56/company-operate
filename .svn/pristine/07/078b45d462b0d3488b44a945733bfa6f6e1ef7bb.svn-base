	color: white !important;
	font-size: 14px;
	border-radius: 3px;
}

.orderOrganEditClose {
	padding: 6px 31px;
	margin-left: 30px;
	background-color: #E8E7E4;
	color: white !important;
	border: 1px solid #E8E7E4;
	font-size: 14px;
	border-radius: 3px;
}

.orderOrganDiv .nav-tabs>li>a,
.withdrawalsDiv .nav-tabs>li>a {
	background-color: white;
	border: 1px solid #57A2F0;
	/*padding: 10px 56px;*/
	color: #57A2F0;
	border-radius: 4px 0 0 4px;
	cursor: pointer;
}

.orderOrganDiv .nav-tabs>li.active>a,
.withdrawalsDiv .nav-tabs>li.active>a {
	background-color: #0099C2;
	color: white;
	/*padding: 10px 56px;*/
	border: 1px solid #0099C2;
}

.orderOrganDiv .nav-tabs,
.withdrawalsDiv .nav-tabs {
	border-bottom: 0;
}

